{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "plugin:prettier/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": ["**/*.test.js"],
    "rules": {
        "react-hooks/exhaustive-deps": "off",
        "react/prop-types": "off",
        "react/jsx-filename-extension": "off"
    }
}
